        _G.Rebirth11 = Value11
        Rebirth11()
    end,
 })

 local Toggle = ClickTab:CreateToggle({
    Name = "‚ôª Auto Rebirth 20",
    CurrentValue = false,
    Flag = "Rebirth20", 
